<body class="body-guitar">
　　　　<div class="text-center">
　　　　　　　<%= form_for(@gsound) do |f| %>
            <div class="search-box">
                <span class="search-i"><%= f.select :g_bpm, [["100", "100"],["120", "120"],["140","140"],["160","160"]] %></span>
                <span class="search-i"><%= f.select :g_key, [["A", "A"],["B", "B"],["C","C"],["D","D"],["E","E"],["F","F"],["G","G"]] %></span>
                <span class="search-i"><%= f.select :g_part, [["rythm", "rythm"],["lead", "lead"]] %></span>
            </div>
                <p class="new-audio"><%= f.file_field :g_sound, class: "g"%></p>
　　　　　　　　<p class="new-body"><%= f.text_area :g_body %></p>

　　　　　　　　<div class="text-center"><button type=submit class="btn btn-danger">Update</button></div>
　　　　　　　<% end %>
　　　　</div>
</body>